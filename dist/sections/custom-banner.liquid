    {
      "type": "image_picker",
      "id": "homepage_banner_style_1",
      "label": "Banner Image Desktop",
      "info": "1440x500"
    },
    {
      "type": "image_picker",
      "id": "tablet_banner_style_1",
      "label": "Banner Image Tablet",
      "info": "1019x500"
    },
    {
      "type": "image_picker",
      "id": "mobile_banner_style_1",
      "label": "Banner Mobile Image",
      "info": "750x300"
    },
    {
      "type": "checkbox",
      "id": "enable_custom_banner_height",
      "label": "Enable Custom Banner Height",
      "default": false
    },
    {
      "type": "range",
      "id": "banner_max_height",
      "min": 200,
      "max": 800,
      "step": 10,
      "unit": "px",
      "label": "Desktop Banner Height",
      "default": 500
    },
    {
      "type": "range",
      "id": "banner_max_height_tablet",
      "min": 200,
      "max": 800,
      "step": 10,
      "unit": "px",
      "label": "Tablet Banner Height",
      "default": 430
    },
    {
      "type": "range",
      "id": "banner_max_height_mobile",
      "min": 50,
      "max": 800,
      "step": 10,
      "unit": "px",
      "label": "Mobile Banner Height",
      "default": 210
