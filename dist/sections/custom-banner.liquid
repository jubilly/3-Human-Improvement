
<!-- start custom-banner.liquid (SECTION) -->
<link rel="stylesheet" data-href="{{ 'section-banner-1.css' | asset_url }}"/>

{%- assign enable_lazy = section.settings.enable_lazy_load | default: false -%}
{% capture block_reviews %}
    {% for block in section.blocks %}
        {% if block.settings.review_content != blank and block.type == 'reviews' %}
            <div class="block-reviews custom-review-{{ forloop.index }}-{{ section.id }} {% if block.settings.block_review_mobile == false %}no-mobile{% endif %}">
                <div class="block-reviews__head">
                    {% if block.settings.review_image != blank %}
                        <div class="block-reviews__author-img">
                            {% render 'responsive-image', 
                                image: block.settings.review_image,
                                id_prefix: forloop.index,
                                custom_class: true,
                                active_lazy: enable_lazy,
                                max_width: 200,
                                no_wrap: true, 
                                alt_text: block.settings.review_image.alt,
                                size_large: 400, 
                                size_mobile: 39, 
                                size_desk: 10 
                                %}
                        </div>
                    {% endif %}
                    <div class="block-reviews__infos">
                        <p class="block-reviews__name">{{ block.settings.review_name }}</p>
                        <div class="block-reviews__stars">
                          {%- for i in (1..block.settings.review_stars) -%}
                            <svg width="12" height="11" viewBox="0 0 12 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M6 0L7.34708 4.1459H11.7063L8.17963 6.7082L9.52671 10.8541L6 8.2918L2.47329 10.8541L3.82037 6.7082L0.293661 4.1459H4.65292L6 0Z" fill="#ECB42E"/>
                            </svg>
                          {%- endfor -%}
                        </div>
                    </div>
                </div>
                <div class="block-reviews__content">
                    {{ block.settings.review_content }}
                </div>
            </div>
        {% endif %}
    {% endfor %}
{% endcapture %}
{% capture banner_content %}
    <section class="shopify-section banner homepage-style-1 {{ section.settings.custom_class }}">
    {%- assign imageDesktop = section.settings.homepage_banner_style_1 -%}
    {%- assign paddingTopDesktop = imageDesktop.height | plus: 0.0 | divided_by: imageDesktop.width | times: 100.0 -%}
    {% assign imageTablet = section.settings.tablet_banner_style_1 %}
    {%- assign paddingTopTablet = imageTablet.height | plus: 0.0 | divided_by: imageTablet.width | times: 100.0 -%}
    {%- assign imageMobile = section.settings.mobile_banner_style_1 -%}
    {%- assign paddingTopMobile = imageMobile.height | plus: 0.0 | divided_by: imageMobile.width | times: 100.0 -%}

    {%- if section.settings.homepage_banner_style_1 == blank -%}
        {%- assign paddingTopDesktop = 40 -%}
    {%- endif -%}

    {%- if imageTablet == blank -%}
        {%- assign paddintTopTablet = 50 -%}
    {%- endif -%}

    {%- if section.settings.mobile_banner_style_1 == blank -%}
        {%- assign paddingTopMobile = 55 -%}
    {%- endif -%}

    {%- assign image_desktop_width = 1800 -%}
    {%- assign image_desktop_height = 1000 -%}
    {%- if imageDesktop == blank -%}
        {%- assign image_desktop_height = image_desktop_width -%}
    {%- endif -%}
    <div class="banner-img {% if section.settings.enable_custom_banner_height == false %}banner-padding-top-desktop{% endif %}{% if section.settings.enable_custom_banner_height == false %} banner-padding-top-tablet{% endif %}{% if section.settings.enable_custom_banner_height == false %} banner-padding-top-mobile{% endif %}">

        {%- if section.settings.homepage_banner_style_1.alt != blank -%}
        {%- assign image_alt = section.settings.homepage_banner_style_1.alt | strip_html | escape -%}
        {%- elsif section.settings.main_banner_title != blank -%}
        {%- assign image_alt = section.settings.main_banner_title | strip_html | escape -%}
        {%- else -%}
        {%- assign image_alt = "Image without description." -%}
        {%- endif -%}
        <picture>
            {%- assign default_image = section.settings.homepage_banner_style_1 -%}
            {%- assign desktop_widths = '1500,1400,1300,1200,1100,1000,900,800' | split: ',' -%}
            {%- assign tablet_widths = '1200,1100,1000,900,800,700' | split: ',' -%}
            {%- assign mobile_widths = '700,600,500,400' | split: ',' -%}

            <source class="{% if enable_lazy %}lazy2 lazy3{% endif %} banner-image-desktop" media="(min-width: 1020px)" {% if enable_lazy %}data-{% endif %}srcset="
            {%- for desktop_width in desktop_widths -%}
                https:{{- default_image | image_url: width: desktop_width }} {{ desktop_width }}w,
            {%- endfor -%}
            "/>

            {%- if imageTablet != blank -%}
            {%- assign default_image = imageTablet -%}
            {%- endif -%}

            <source class="{% if enable_lazy %}lazy2 lazy3{% endif %} banner-image-tablet" media="(min-width: 768px)" {% if enable_lazy %}data-{% endif %}srcset="
            {%- for tablet_width in tablet_widths -%}
                https:{{- default_image | image_url: width: tablet_width }} {{ tablet_width }}w,
            {%- endfor -%}
            "/>
    
            {%- if section.settings.mobile_banner_style_1 != blank -%}
            {%- assign default_image = section.settings.mobile_banner_style_1 -%}
            {%- endif -%}
    
            <source class="{% if enable_lazy %}lazy2 lazy3{% endif %} banner-image-mobile" media="(max-width: 767px)" {% if enable_lazy %}data-{% endif %}srcset="
            {%- for mobile_width in mobile_widths -%}
                https:{{- default_image | image_url: width: mobile_width }} {{ mobile_width }}w,
            {%- endfor -%}
            "/>
            <img width="{{ image_desktop_width }}" height="{{ image_desktop_height }}"  src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" alt="Banner"/>
        </picture>
        {%- if section.settings.main_banner_title != "" -%}
        <div class="banner-titles">
            {%- if section.settings.enable_h1 -%}
                <h1 class="banner-title">{{ section.settings.main_banner_title }}</h1>
            {%- else -%}
                <h2 class="banner-title">{{ section.settings.main_banner_title }}</h2>
            {%- endif -%}
            <div class="banner-sub-title">{{ section.settings.main_banner_sub_title }}</div>
        </div>
        {%- else -%}
            <h2 class="hide" style="top:{{ section.settings.vasta_title_vertical_position }}%; left:{{ section.settings.vasta_title_horizontal_position }}%">{{ shop.name }}</h2>
            <div class="banner-titles">
            <div class="banner-sub-title" >{{ section.settings.main_banner_sub_title }}</div>
            </div>
        {%- endif -%}
        
        {% assign customText = section.settings.custom_text %}

        {% if customText != blank %}
        <div class="banner_custom_text">
            <p>{{ customText }}</p>
        </div>
        {% endif %}

        {%- assign count = 0 -%}
        <div class="slider-buttons" id="CallToAction-{{ section.id }}">
            {%- for block in section.blocks -%}
            {%- if "select" == block.type -%}
              <style>
              .banner__button--{{ forloop.index }}-{{ section.id }} {
                {{ block.settings.custom_html }}
              }
              </style>

              {%- assign count = count | plus: 1 -%}
              {% if block.settings.enable_helper_text %}<div class="helper-text__content helper-text__content-{{ forloop.index }}-{{ section.id }}">{% endif %}
                {% if block.settings.enable_button_link %}
                  <a {% if block.settings.enable_helper_text == false %}id="Banner1Btn-{{ forloop.index }}-{{ section.id }}"{% else %} style="position: static;transform: unset;"{% endif %}
                    class="banner__button--{{ forloop.index }}-{{ section.id }}
                    {% if block.settings.show_mobile_buttons == false %} no-mobile {% endif %} 
                    btn btn-v {{ section.settings.vasta_buttons_hover_effect }}" 
                    title="{{ block.settings.linktext | strip_html | escape }}" 
                    href="{{ block.settings.link }}">
                    {{ block.settings.linktext }}
                  </a>
                {% else %}
                  <span {% if block.settings.enable_helper_text == false %}id="Banner1Btn-{{ forloop.index }}-{{ section.id }}"{% else %} style="position: static;transform: unset;"{% endif %}
                    class="banner__button--{{ forloop.index }}-{{ section.id }} banner__button--no-link
                    {% if block.settings.show_mobile_buttons == false %} no-mobile {% endif %} 
                    btn btn-v {{ section.settings.vasta_buttons_hover_effect }}" 
                    title="{{ block.settings.linktext | strip_html | escape }}" 
                    href="{{ block.settings.link }}">
                    {{ block.settings.linktext }}
                  </span>
                {% endif %}
              {% if block.settings.enable_helper_text %}
                <span class="custom__help-text">
                  {{ block.settings.helper_text }}
                </span>
              {% endif %}
            {% if block.settings.enable_helper_text %}</div>{% endif %}
            {%- endif -%}
        {%- endfor -%}
        </div>
        {{ block_reviews }}
    </div>
    </section>
{% endcapture %} 

{% if section.settings.full_with_banne1 %}
  {{ banner_content }}
{% else %}
  <div class="wrapper">
  {{ banner_content }}
  </div>
{% endif %}

{% schema %}
{
  "name": "Custom Banner",
  "class": "banner",
  "max_blocks": 50,
  "settings": [
    {
      "type": "header",
      "content": "General Settings"
