    },
    {
      "type": "select",
      "id": "vasta_headline_align",
      "label": "Headline Alignment",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "right",
          "label": "Right"
        }
      ],
      "default": "center"
    }, 
    {
      "type": "select",
      "id": "text_transform",
